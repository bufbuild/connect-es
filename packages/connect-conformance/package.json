{
  "name": "@connectrpc/connect-conformance",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "buf generate buf.build/connectrpc/conformance:v1.0.2",
    "clean": "rm -rf ./dist/*",
    "build": "tsc --project tsconfig.json --module ES2015 --verbatimModuleSyntax --outDir ./dist --declaration --declarationDir ./dist",
    "test:web:chrome": "./bin/connectconformance.js --mode client --conf conformance-web.yaml -v -- ./bin/conformancewebclient.js --browser chrome --headless",
    "test:web:firefox": "./bin/connectconformance.js --mode client --conf conformance-web.yaml -v --known-failing '**/server-stream/cancel-after-zero-responses' --known-failing '**/server-stream/cancel-after-responses' -- ./bin/conformancewebclient.js --browser firefox --headless",
    "test:web:safari": "./bin/connectconformance.js --mode client --conf conformance-web.yaml -v -- ./bin/conformancewebclient.js --browser safari --headless",
    "test:web:node": "./bin/connectconformance.js --mode client --conf conformance-web-node.yaml -v -- ./bin/conformancewebclient.js --browser node",
    "test:web": "./bin/connectconformance.js --mode client --conf conformance-web.yaml -v -- ./bin/conformancewebclient.js",
    "test:cloudflare:server": "npx wrangler deploy -c wrangler-server.toml && ./bin/connectconformance.js --mode server --conf conformance-cloudflare-server.yaml -v ./bin/conformancecloudflareserver.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@bufbuild/protobuf": "^1.7.2",
    "@connectrpc/connect": "1.4.0",
    "@connectrpc/connect-node": "1.4.0",
    "fflate": "^0.8.1",
    "tar-stream": "^3.1.7",
    "undici": "^5.28.4",
    "webdriverio": "^8.36.1",
    "esbuild": "^0.20.0"
  },
  "devDependencies": {
    "@types/node-forge": "^1.3.9",
    "@types/tar-stream": "^3.1.3",
    "@types/debug": "^4.1.12",
    "@cloudflare/workers-types": "^4.20240329.0",
    "wrangler": "^3.53.0"
  }
}
