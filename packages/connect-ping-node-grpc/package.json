{
  "private": true,
  "type": "module",
  "engineStrict": true,
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "clean": "rm -rf ./dist/esm/* ./dist/types/*",
    "build": "npx tsc --project tsconfig.json --module ES2015 --outDir ./dist/esm --declaration --declarationDir ./dist/types",
    "client:verbose": "npm run build && GRPC_TRACE=call_stream GRPC_VERBOSITY=DEBUG node dist/esm/client.js",
    "client": "npm run build && node dist/esm/client.js",
    "server": "npm run build && GRPC_TRACE=call_stream GRPC_VERBOSITY=DEBUG node dist/esm/server.js"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^0.0.4",
    "@grpc/grpc-js": "^1.6.7"
  },
  "devDependencies": {
    "typescript": "^4.7.2"
  }
}
