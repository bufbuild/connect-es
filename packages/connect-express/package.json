{
  "name": "@connectrpc/connect-express",
  "version": "1.4.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/connectrpc/connect-es.git",
    "directory": "packages/connect-express"
  },
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.json --module commonjs --outDir ./dist/cjs --declaration --declarationDir ./dist/cjs && echo >./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "tsc --project tsconfig.json --module ES2015 --verbatimModuleSyntax --outDir ./dist/esm --declaration --declarationDir ./dist/esm",
    "attw": "attw --pack",
    "conformance": "npm run conformance:generate && npm run conformance:build && npm run conformance:run",
    "conformance:build": "tsc --project tsconfig.conformance.json --module ES2015 --verbatimModuleSyntax --outDir ./conformance/dist",
    "conformance:generate": "buf generate buf.build/connectrpc/conformance:v1.0.2 --template conformance/buf.gen.yaml",
    "conformance:run": "./conformance/bin/connectconformance --mode server --conf ./conformance/conformance-node.yaml -v ./conformance/bin/conformanceserver"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@types/express": "^4.17.18",
    "@bufbuild/protobuf": "^1.7.2",
    "@connectrpc/connect": "1.4.0",
    "@connectrpc/connect-node": "1.4.0",
    "fflate": "^0.8.1",
    "tar-stream": "^3.1.7",
    "undici": "^5.28.4"
  },
  "devDependencies": {
    "@types/tar-stream": "^3.1.3",
    "@types/debug": "^4.1.12"
  },
  "files": [
    "dist/**"
  ]
}
