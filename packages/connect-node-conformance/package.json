{
  "name": "@connectrpc/connect-node-conformance",
  "private": true,
  "bin": {
    "conformanceserver": "bin/conformanceserver",
    "conformanceclient": "bin/conformanceclient",
    "connectconformance": "bin/connectconformance"
  },
  "scripts": {
    "generate": "buf generate buf.build/connectrpc/conformance:v1.0.0-rc1",
    "clean": "rm -rf ./dist/cjs/*",
    "build": "tsc --project tsconfig.json --module commonjs --outDir ./dist/cjs",
    "test:server": "./bin/connectconformance --mode server --conf conformance.yaml -v ./bin/conformanceserver",
    "test:client": "./bin/connectconformance --mode client --conf conformance.yaml -v ./bin/conformanceclient"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.4.2",
    "@connectrpc/connect": "1.1.4",
    "@connectrpc/connect-node": "1.1.4",
    "node-forge": "^1.3.1",
    "asn1js": "^3.0.5",
    "fflate": "^0.8.1",
    "tar-stream": "^3.1.6",
    "undici": "^5.26.2"
  },
  "devDependencies": {
    "@types/node-forge": "^1.3.9",
    "@types/tar-stream": "^3.1.3"
  }
}
